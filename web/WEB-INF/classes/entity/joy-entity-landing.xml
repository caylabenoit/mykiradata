        <joy-entity alias="X">APP_PLUGINS</joy-entity>
        <joy-join type="INNER">
            <joy-entity alias="Y">APP_PLUGINS_TYPE</joy-entity>
            <joy-join-key master="APT_FK" slave="APT_PK" />
        </joy-join>
    </joy-composite>
    
</joy-entities>
